/* <!--wxss部分--> */

.container {
  position: relative;
}

.canvasStyle {
  width: 100%;
  height: 500px;
  margin: 20rpx auto;
  position: relative;
  background: #fff;
}

.canvasStyle canvas {
  width: 100%;
  height: 100%;
  margin: 1px 0 0 1px;
  background: #fff;
  position: relative;
  z-index: 10;
  font-family: "Times New Roman", Georgia, Serif;
}

.vol-swiper-item {
  padding: 25rpx;
  box-sizing: border-box;
}

.vol-card {
  padding: 25rpx;
  border: 1px solid #fafafa;
  border-radius: 5px;
  background-color: #fff;
  /* box-shadow: 1px 1px 1px #ebebeb;  */
  position: absolute;
}

/* .vol-card{width:200px;height:310px;position:absolute;top:1px;left:1px;z-index:1;} */




.chooseImage {
  width: 90%;
  padding: 30rpx 5% 0 5%;
}

.BtnImg_select {
  height: 80rpx;
  line-height: 80rpx;
}

.BtnImg_select:active {
  color: #f1f1f1;
  background-color: #d95649;
}

.Input_text {
   margin: 20rpx 0;
  border: 1rpx solid #e6e6e6;
  border-radius: 6rpx;
  font-size: 30rpx;
  height: 80rpx;

}

.Input_text textarea  {
  width: 100%;
  height: 100%;
  padding:0 20rpx;
}

